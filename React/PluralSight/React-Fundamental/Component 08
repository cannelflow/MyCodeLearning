//js file
var Hello = React.createClass({
    propType:{
        data:React.PropTypes.string.isRequired,
        data1:function(props,propName,componentName){
            if(props[propName]<5){
                throw new Error(propName+
                "must be 5 or greator"
                )
            }
        }
    },
    render:function(){
        return(
            <div>
                <h1>Hello At {this.props.data} And {this.props.data1}</h1>
            </div>
        );
    }
});
ReactDOM.render(
    <Hello data={new Date().toString()} data1={4}/>,
    document.querySelector("#app")
);
