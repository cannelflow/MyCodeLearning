//js file

var First = React.createClass({
    render:function(){
        return(
            <div>
                <h1>Hello From First Component</h1>
            </div>
        );
    },
});
var Second = React.createClass({
    render:function(){
        return(
            <div>
                <h1>Hello From Second Component</h1>
            </div>
        );
    }
});
var Appander = React.createClass({
    render:function(){
        return(
            <div>
                {this.props.children}
            </div>
        );
    }
});
ReactDOM.render(
    <div>
           <Appander>
               <First />
               <Second />
           </Appander>
    </div>,
    document.querySelector("#app")
);
