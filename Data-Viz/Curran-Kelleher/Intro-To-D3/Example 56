//js file
//create svg element by selecting the body and appending svg
//with height & width
var svg = d3.select("body").append("svg")
                           .attr("height", 250)
                           .attr("width", 250);
/*
to create rect append rect to svg and give height & width
*/
var rect = svg.append("rect")
              .attr("height", 100)
              .attr("width", 100)
              .attr("x", 50)
              .attr("y", 50)
              .attr("fill", "blue");
