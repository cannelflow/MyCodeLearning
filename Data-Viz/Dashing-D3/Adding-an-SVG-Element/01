var body = d3.select("body");
var svg = body.append("svg")
    .attr("width", 50)
    .attr("height", 50);
var circle = svg.append("circle")
    .attr("cx", 25)
    .attr("cy", 25)
    .attr("r", 25)
    .style("fill", "red");

